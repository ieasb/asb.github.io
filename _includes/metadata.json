{
  "@context": "http://schema.org",
  "@type": "Organization",
  "name": "{{ site.title }}",
  "url": "{{ '' | absolute_url }}",
  "mainEntityOfPage": "{{ page.url | replace:'index.html','' | absolute_url }}",
  "description": "site.meta_description",
  "image": {
    "@type": "ImageObject",
    "url": "{{ site.logo | absolute_url }}",
    "height": "60",
    "width": "60"
  },
  "sameAs": [
      "https://facebook.com/{{ site.facebook }}",
      "https://twitter.com/{{ site.twitter }}",
      "https://instagram.com/{{ site.instagram }}",
      "https://youtube.com/{{ site.youtube }}"
  ]
}
